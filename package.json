{
  "name": "shopify-hide-price-app",
  "version": "1.0.0",
  "private": true,
  "description": "Shopify app to hide product prices when out of stock",
  "scripts": {
    "build": "remix vite:build",
    "dev": "shopify app dev",
    "deploy": "shopify app deploy",
    "config:link": "shopify app config link",
    "generate": "shopify app generate",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy",
    "typecheck": "tsc",
    "docker-start": "npm run setup && npm run start",
    "start": "remix-serve ./build/server/index.js"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "@remix-run/node": "^2.7.1",
    "@remix-run/react": "^2.7.1",
    "@remix-run/serve": "^2.7.1",
    "@shopify/app-bridge-react": "^4.1.2",
    "@shopify/polaris": "^12.27.0",
    "@shopify/shopify-api": "^9.7.2",
    "@shopify/shopify-app-remix": "^2.8.2",
    "@shopify/shopify-app-session-storage-prisma": "^4.0.5",
    "isbot": "^5.1.2",
    "prisma": "^5.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^7.6.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.7.1",
    "@shopify/app": "^3.58.0",
    "@shopify/cli": "^3.58.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "workspaces": [
    "extensions/*"
  ],
  "trustedDependencies": [
    "@shopify/app",
    "@shopify/cli"
  ],
  "resolutions": {
    "undici": "6.6.1"
  },
  "overrides": {
    "undici": "6.6.1"
  }
}
